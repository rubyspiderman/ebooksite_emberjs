{{view ESF.LogoView}}

{{#if phone}}
  <span class="phone">{{phone}}</span>
{{/if}}

{{#if mail}}
  <a {{bind-attr href=emailLink}} class="mail">{{t "school.mail"}}</a>
{{/if}}

{{#if url}}
  <a {{bind-attr href=url}} class="url" target="_blank">{{t "school.url"}}</a>
{{/if}}

{{#if facebook}}
  <a {{bind-attr href=facebookLink}} class="facebook" target="_blank">{{t "school.facebook"}}</a>
{{/if}}

{{#if twitter}}
  <a {{bind-attr href=twitterLink}} class="twitter" target="_blank">{{t "school.twitter"}}</a>
{{/if}}
